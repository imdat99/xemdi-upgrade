{
  "name": "xemdi-upgrade",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx --conditions=react-server src/server",
    "dev:client": "vite",
    "build": "yarn run clean && yarn build:client && yarn build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server1": "vite build --outDir dist/server --ssr ./entry-server.tsx",
    "build:server": "tsc -p tsconfig.server.json && vite build --ssr ./src/entry-server.tsx --outDir dist/server --minify",
    "preview": "yarn build && cross-env NODE_ENV=production yarn tsx server",
    "build:node-server": "vite build -c vite-node.config.ts --mode optimized",
    "serve:win": "cross-env NODE_ENV=production --conditions=react-server node dist/app/server.js",
    "serve": "NODE_ENV=production node dist/app/server.js",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@types/node": "^22.5.5",
    "@types/serve-static": "^1.15.7",
    "artplayer": "^5.2.1",
    "cross-env": "^7.0.3",
    "h3": "^1.12.0",
    "hls.js": "^1.5.17",
    "isomorphic-fetch": "^3.0.0",
    "react": "^19.0.0-beta-26f2496093-20240514",
    "react-dom": "^19.0.0-beta-26f2496093-20240514",
    "react-helmet-async": "^2.0.4",
    "react-refresh": "^0.14.2",
    "react-router-dom": "^6.26.2",
    "react-server-dom-webpack": "19.0.0-beta-26f2496093-20240514",
    "redis": "^4.7.0",
    "rimraf": "^6.0.1",
    "serve-static": "^1.16.2",
    "swiper": "^11.1.14",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.9.0",
    "@originjs/vite-plugin-federation": "^1.3.6",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "babel-plugin-react-compiler": "^19.0.0-beta-8a03594-20241020",
    "eslint": "^9.9.0",
    "eslint-plugin-react-compiler": "^19.0.0-beta-8a03594-20241020",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jiti": "^1.21.6",
    "react-compiler-runtime": "^19.0.0-beta-8a03594-20241020",
    "tsx": "^4.19.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-babel": "^1.2.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
  }
}
